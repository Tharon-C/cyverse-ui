{
  "name": "cyverse-ui",
  "version": "0.0.3",
  "description": "A React UI library for the CyVerse tool set",
  "main": "./index.js",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui"
  ],
  "scripts": {
    "build": "babel -d . src; node ./src/scripts/makeTheme.js; node scripts/makeDocs.js; cp ./src/styles/animation.css ./styles",
    "lint": "eslint -c .eslintrc --ext .jsx,.js --no-eslintrc src",
    "watch": "babel -w -d . src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyverse/cyverse-ui.git"
  },
  "author": "CyVerse",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/cyverse/cyverse-ui/issues"
  },
  "homepage": "https://github.com/cyverse/cyverse-ui#readme",
  "peerDependencies": {
    "material-ui": "~0.17.1",
    "react": "^15.0.1",
    "react-tap-event-plugin": "~2.0.1"
  },
  "dependencies": {
    "jss": "^7.1.5",
    "jss-preset-default": "^2.0.0",
    "jss-theme-reactor": "^0.11.1",
    "prop-types": "^15.5.10",
    "react-css-stagger": "0.0.5",
    "react-ink": "6.2.0",
    "react-scroll": "1.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "eslint": "^2.13.*",
    "eslint-plugin-react": "^5.1.1",
    "material-ui": "~0.17.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-docgen": "^2.17.0",
    "react-tap-event-plugin": "~2.0.1",
    "write-json": "^1.0.1"
  }
}
